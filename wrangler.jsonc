{
    "name": "aia-cdp-worker",
    "main": "src/worker.ts",
    "compatibility_date": "2025-09-16",
    "build": {
        "command": "npm run build",
        "watch_dir": "src/frontend"
    },
    "r2_buckets": [
        {
            "binding": "STORE",
            "bucket_name": "aia-cdp"
        }
    ],
    "vars": {
        "SITE_NAME": "PKI AIA/CDP"
    },
    "observability": {
        "logs": {
            "enabled": true
        }
    },
    "assets": {
        "directory": "./public"
    },
    "env": {
        "dev": {
            "name": "aia-cdp-worker-dev",
            "workers_dev": true,
            "r2_buckets": [
                {
                    "binding": "STORE",
                    "bucket_name": "aia-cdp",
                    "preview_bucket_name": "aia-cdp-dev"
                }
            ],
            "vars": {
                "SITE_NAME": "PKI AIA/CDP (Dev)"
            },
            "build": {
                "command": "npm run build",
                "watch_dir": "src/frontend"
            },
            "observability": {
                "logs": {
                    "enabled": true
                }
            }
        }
    }
}