{
    "name": "aia-cdp-worker",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:dev": "vite build --mode development",
        "dev": "npm-run-all --parallel dev:assets dev:worker",
        "dev:assets": "vite build --mode development --watch",
        "dev:frontend": "vite",
        "dev:worker": "wrangler dev --env local",
        "deploy": "wrangler deploy",
        "preview": "vite preview",
        "reset:dev": "bash scripts/reset-dev.sh",
        "seed:dev": "bash scripts/seed-dev.sh",
        "test": "tsx --test tests/**/*.test.ts"
    },
    "dependencies": {
        "asn1js": "^3.0.7",
        "pkijs": "^3.3.3",
        "pvutils": "^1.1.5"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260203.0",
        "@types/node": "^25.2.0",
        "@vitejs/plugin-legacy": "^7.2.1",
        "npm-run-all": "^4.1.5",
        "linkedom": "^0.18.12",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "wrangler": "^4.62.0"
    }
}